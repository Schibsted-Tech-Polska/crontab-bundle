{
    "name": "schibsted-tech-polska/crontab-bundle",
    "description": "Crontab Bundle",
    "keywords": [
        "crontab",
        "bundle"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Stp\\CrontabBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stp\\CrontabBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",

        "cache/predis-adapter": "^0.4",
        "psr/log": "^1.0",
        "schibsted-tech-polska/crontab": "^1.0",
        "symfony/config": "^2.8 || ^3.0",
        "symfony/console": "^2.8 || ^3.0",
        "symfony/dependency-injection": "^2.8 || ^3.0",
        "symfony/http-kernel": "^2.8 || ^3.0",
        "symfony/process": "^2.8 || ^3.0",

        "cache/adapter-bundle": "^0.4"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.0"
    }
}
